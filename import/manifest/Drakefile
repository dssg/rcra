; get new york manifest data
!$[DATA_DIR]/import/manifest/ <- [-timecheck]
    mkdir -p $OUTPUT
    python import/manifest/manifest_download.py $OUTPUT

; clean ny manifest data 
!$[DATA_DIR]/import/manifest/clean <- !$[DATA_DIR]/import/manifest/ [-timecheck] 
    mkdir -p $OUTPUT
    import/manifest/manifest_cleaning.sh $INPUT $OUTPUT

; copy ny manifest data to db
!$[SQL_DIR]/manifest <- !$[DATA_DIR]/import/manifest/clean
    psql -v ON_ERROR_STOP=1 -f import/manifest/manifest_cleaning.sql



