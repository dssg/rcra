    $INPUT $INPUT1 4 > $OUTPUT

!$[DATA_DIR]/import/tri/tri.type_5.csv <- import/tri/create_tri_csv.sh, !$[DATA_DIR]/import/tri/ [-timecheck]
   $INPUT $INPUT1 5 > $OUTPUT

!$[DATA_DIR]/import/tri/tri.type_6.csv <- import/tri/create_tri_csv.sh, !$[DATA_DIR]/import/tri/ [-timecheck]
   $INPUT $INPUT1 6 > $OUTPUT

