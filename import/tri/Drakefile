!$[DATA_DIR]/import/tri/ <- [-timecheck]   
    for i in {1987..2013}; do
        wget -N -P $OUTPUT http://www3.epa.gov/tri/US_${i}_v13.zip
    done
    unzip -d $OUTPUT $OUTPUT/\*.zip

!$[DATA_DIR]/import/tri/tri.type_1.csv <- import/tri/create_tri_csv.sh, !$[DATA_DIR]/import/tri/ [-timecheck]
    $INPUT $INPUT1 1 > $OUTPUT

!$[DATA_DIR]/import/tri/tri.type_2a.csv <- import/tri/create_tri_csv.sh, !$[DATA_DIR]/import/tri/ [-timecheck]
    $INPUT $INPUT1 "2a" > $OUTPUT

!$[DATA_DIR]/import/tri/tri.type_2b.csv <- import/tri/create_tri_csv.sh, !$[DATA_DIR]/import/tri/ [-timecheck]
    $INPUT $INPUT1 "2b" > $OUTPUT

