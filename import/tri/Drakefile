    $INPUT $INPUT1 "3a" > $OUTPUT

!$[DATA_DIR]/import/tri/tri.type_3b.csv <- import/tri/create_tri_csv.sh, !$[DATA_DIR]/import/tri/ [-timecheck]
    $INPUT $INPUT1 "3b" > $OUTPUT

