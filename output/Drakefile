$[PSQL_DIR]/output/ <- [method:psql_schema -timecheck]

$[DATA_DIR]/investigations.sql <- output/investigations_sql.py, output/investigations.sql, $[PSQL_DIR]/rcra/rcra
    python $INPUT > $OUTPUT

$[PSQL_DIR]/output/investigations <- $[DATA_DIR]/investigations.sql [method:psql]

$[PSQL_DIR]/output/region_states <- output/create_region_states.sql [method:psql]
$[PSQL_DIR]/output/handlers <- output/handlers.sql, $[PSQL_DIR]/rcra/rcra [method:psql]
$[PSQL_DIR]/output/facilities <- output/facilities.sql, $[PSQL_DIR]/rcra/rcra, $[PSQL_DIR]/rcra/rcra_facilities [method:psql]

$[PSQL_DIR]/output/br <- output/br.sql, $[PSQL_DIR]/br [method:psql]

; takes 15 minutes to run
$[PSQL_DIR]/output/facility_years0101 <- output/facility_years.py, output/facility_years.sql, $[PSQL_DIR]/output/investigations, $[PSQL_DIR]/output/facilities, $[PSQL_DIR]/output/handlers
    python $INPUT $INPUT1 2005 2016 01 01 | psql && touch $OUTPUT
