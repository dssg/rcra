$[SQL_DIR]/output/ <- [method:psql_schema -timecheck]

$[DATA_DIR]/investigations.sql <- output/investigations_sql.py, output/investigations.sql, $[SQL_DIR]/rcra/rcra, $[SQL_DIR]/indexes
    python $INPUT > $OUTPUT

$[SQL_DIR]/output/region_states <- output/region_states.sql [method:psql]

$[SQL_DIR]/output/investigations <- $[DATA_DIR]/investigations.sql, $[SQL_DIR]/output/region_states [method:psql]

$[SQL_DIR]/output/handlers <- output/handlers.sql, $[SQL_DIR]/rcra/rcra [method:psql]
$[SQL_DIR]/output/facilities <- output/facilities.sql, $[SQL_DIR]/rcra/rcra, $[SQL_DIR]/rcra/rcra_facilities, $[SQL_DIR]/output/region_states [method:psql]

; date_floor used by facility_years
$[SQL_DIR]/util <- output/util.sql [method:psql]

; takes 15 minutes to run
$[SQL_DIR]/output/facility_years0101 <- output/facility_years.py, output/facility_years.sql, $[SQL_DIR]/output/investigations, $[SQL_DIR]/output/facilities, $[SQL_DIR]/output/handlers, $[SQL_DIR]/util
    python $INPUT $INPUT1 2005 2016 01 01 | psql -v ON_ERROR_STOP=1 && echo > $OUTPUT

$[SQL_DIR]/output/br <- output/br.sql, $[SQL_DIR]/br [method:psql]

$[SQL_DIR]/output/icis_fec <- output/icis_fec.sql, $[SQL_DIR]/icis/fec [method:psql]

$[SQL_DIR]/output/rmp <- output/rmp.sql, $[SQL_DIR]/rmp, $[SQL_DIR]/frs [method:psql]

$[SQL_DIR]/output/nysdec_reports <- output/nysdec_reports.sql [method:psql]
